{
  "name": "create-nx-fhir",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/create-nx-fhir/bin",
  "projectType": "library",
  "release": {
    "version": {
      "manifestRootsToUpdate": ["dist/{projectRoot}"],
      "currentVersionResolver": "git-tag",
      "fallbackCurrentVersionResolver": "disk"
    }
  },
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/create-nx-fhir",
        "main": "packages/create-nx-fhir/bin/index.ts",
        "tsConfig": "packages/create-nx-fhir/tsconfig.lib.json",
        "assets": [
          "packages/create-nx-fhir/*.md",
          {
            "input": "./packages/create-nx-fhir",
            "glob": "package.json",
            "output": "."
          }
        ]
      }
    },
    "nx-release-publish": {
      "options": {
        "packageRoot": "dist/{projectRoot}"
      }
    },
    "unpublish": {
      "command": "npm unpublish create-nx-fhir@$(node -p \"require('./packages/create-nx-fhir/package.json').version\") --force"
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "configFile": "packages/create-nx-fhir/vite.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  },
  "implicitDependencies": ["nx-fhir"]
}
