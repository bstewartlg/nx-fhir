{
  "name": "@nx-fhir/source",
  "$schema": "node_modules/nx/schemas/project-schema.json",
  "targets": {
    "local-registry": {
      "executor": "@nx/js:verdaccio",
      "options": {
        "port": 4873,
        "config": ".verdaccio/config.yml",
        "storage": "tmp/local-registry/storage"
      }
    },
    "build": {
      "executor": "nx:noop",
      "cache": true,
      "outputs": []
    },
    "test": {
      "executor": "nx:noop",
      "cache": true,
      "outputs": []
    }
  }
}
